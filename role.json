{
    "cpp": [
        {
            "name": "keyword",
            "pattern": "\\bint\\b|\\bfloat\\b|\\bdouble\\b|\\bchar\\b|\\bvoid\\b|\\breturn\\b|\\bif\\b|\\belse\\b|\\bwhile\\b|\\bfor\\b|\\bclass\\b|\\bpublic\\b|\\bprivate\\b|\\bprotected\\b",
            "format": {
                "foreground": "yellow",
                "fontWeight": "bold"
            }
        },
        {
            "name": "comment",
            "pattern": "//[^\\n]*",
            "format": {
                "foreground": "darkGreen"
            }
        },
        {
            "name": "string",
            "pattern": "\".*?\"",
            "format": {
                "foreground": "darkMagenta"
            }
        },
        {
            "name": "function",
            "pattern": "\\b(?!int\\b|float\\b|double\\b|char\\b|void\\b|return\\b|if\\b|else\\b|while\\b|for\\b|class\\b|public\\b|private\\b|protected\\b)[A-Za-z0-9_]+(?=\\()",
            "format": {
                "foreground": "blue"
            }
        }
    ],
    "java": [
        {
            "name": "keyword",
            "pattern": "\\babstract\\b|\\bassert\\b|\\bboolean\\b|\\bbreak\\b|\\bbyte\\b|\\bcase\\b|\\bcatch\\b|\\bchar\\b|\\bclass\\b|\\bconst\\b|\\bcontinue\\b|\\bdefault\\b|\\bdo\\b|\\bdouble\\b|\\belse\\b|\\benum\\b|\\bextends\\b|\\bfinal\\b|\\bfinally\\b|\\bfloat\\b|\\bfor\\b|\\bgoto\\b|\\bif\\b|\\bimplements\\b|\\bimport\\b|\\binstanceof\\b|\\bint\\b|\\binterface\\b|\\blong\\b|\\bnative\\b|\\bnew\\b|\\bpackage\\b|\\bprivate\\b|\\bprotected\\b|\\bpublic\\b|\\breturn\\b|\\bshort\\b|\\bstatic\\b|\\bstrictfp\\b|\\bsuper\\b|\\bswitch\\b|\\bsynchronized\\b|\\bthis\\b|\\bthrow\\b|\\bthrows\\b|\\btransient\\b|\\btry\\b|\\bvoid\\b|\\bvolatile\\b|\\bwhile\\b",
            "format": {
                "foreground": "darkBlue",
                "fontWeight": "bold"
            }
        },
        {
            "name": "singleLineComment",
            "pattern": "//[^\\n]*",
            "format": {
                "foreground": "darkGreen"
            }
        },
        {
            "name": "multiLineComment",
            "pattern": "/\\*.*?\\*/",
            "format": {
                "foreground": "darkGreen"
            }
        },
        {
            "name": "string",
            "pattern": "\".*?\"",
            "format": {
                "foreground": "darkMagenta"
            }
        },
        {
            "name": "function",
            "pattern": "\\b[A-Za-z0-9_]+(?=\\()",
            "format": {
                "foreground": "blue"
            }
        },
        {
            "name": "annotation",
            "pattern": "@[A-Za-z0-9_]+",
            "format": {
                "foreground": "darkCyan"
            }
        }
    ],
    "python": [
        {
            "name": "keyword",
            "pattern": "\\bFalse\\b|\\bNone\\b|\\bTrue\\b|\\band\\b|\\bas\\b|\\bassert\\b|\\bbreak\\b|\\bclass\\b|\\bcontinue\\b|\\bdef\\b|\\bdel\\b|\\belif\\b|\\belse\\b|\\bexcept\\b|\\bfinally\\b|\\bfor\\b|\\bfrom\\b|\\bglobal\\b|\\bif\\b|\\bimport\\b|\\bin\\b|\\bis\\b|\\blambda\\b|\\bnonlocal\\b|\\bnot\\b|\\bor\\b|\\bpass\\b|\\braise\\b|\\breturn\\b|\\btry\\b|\\bwhile\\b|\\bwith\\b|\\byield\\b",
            "format": {
                "foreground": "darkBlue",
                "fontWeight": "bold"
            }
        },
        {
            "name": "singleLineComment",
            "pattern": "#[^\\n]*",
            "format": {
                "foreground": "darkGreen"
            }
        },
        {
            "name": "multiLineComment",
            "pattern": "\"\"\".*?\"\"\"|'''.*?'''",
            "format": {
                "foreground": "darkGreen"
            }
        },
        {
            "name": "string",
            "pattern": "\".*?\"|'.*?'",
            "format": {
                "foreground": "darkMagenta"
            }
        },
        {
            "name": "function",
            "pattern": "\\bdef\\s+([A-Za-z0-9_]+)\\b",
            "format": {
                "foreground": "blue"
            }
        },
        {
            "name": "class",
            "pattern": "\\bclass\\s+([A-Za-z0-9_]+)\\b",
            "format": {
                "foreground": "darkCyan",
                "fontWeight": "bold"
            }
        },
        {
            "name": "decorator",
            "pattern": "@[A-Za-z0-9_]+",
            "format": {
                "foreground": "darkYellow"
            }
        }
    ]
}