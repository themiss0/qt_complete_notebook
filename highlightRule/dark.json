{
    "cpp": [
        {
            "name": "keyword",
            "pattern": "\\bint\\b|\\bfloat\\b|\\bdouble\\b|\\bchar\\b|\\bvoid\\b|\\breturn\\b|\\bif\\b|\\belse\\b|\\bwhile\\b|\\bfor\\b|\\bclass\\b|\\bpublic\\b|\\bprivate\\b|\\bprotected\\b|\\bnew\\b|\\bdelete\\b|\\btry\\b|\\bcatch\\b|\\bthrow\\b|\\bnamespace\\b|\\busing\\b|\\btrue\\b|\\bfalse\\b",
            "format": {
                "foreground": "#569CD6",
                "fontWeight": "bold"
            }
        },
        {
            "name": "singleLineComment",
            "pattern": "//[^\\n]*",
            "format": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "multiLineComment",
            "pattern": "/\\*.*?\\*/",
            "format": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "string",
            "pattern": "\".*?\"",
            "format": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "function",
            "pattern": "\\b[A-Za-z0-9_]+(?=\\()",
            "format": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "preprocessor",
            "pattern": "#.*",
            "format": {
                "foreground": "#9B9B9B"
            }
        },
        {
            "name": "number",
            "pattern": "\\b\\d+(\\.\\d+)?\\b",
            "format": {
                "foreground": "#B5CEA8"
            }
        }
    ],
